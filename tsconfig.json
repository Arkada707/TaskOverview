{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "emitDeclarationOnly": false,
    "types": ["cypress"]
  },
  "vueCompilerOptions": {
    "target": "auto"
  },
  "include": ["**/*.*"]
}
